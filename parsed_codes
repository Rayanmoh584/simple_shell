struct cmd {
    int type ;
};
struct EXEC{
    int type;
    char eargv [maximumargs];
    char *argv [maximumargs];
};
struct REDIR{
    int type;
    struct cmd *cmd;
    char *file;
    char *efile;
    int mode;
    int fd;
};
//pipe is not an allowed function that's why has been skipped.
struct LIST{
    int type;
    struct cmd *left;
    struct cmd *right;
};
struct BACK{
    int type;
    struct cmd *CMD;
};